{
  "name": "schedule",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "db:script": "npx tsx prisma/script.ts",
    "db:setup": "prisma generate && prisma migrate deploy && npm run db:seed",
    "db:seed": "npx tsx prisma/script.ts --seeding true",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "format": "prettier --write \"app/**/*.{js,ts,jsx,tsx}\" \"tests/**/*.{js,ts,jsx,tsx}\" \"types.d.ts\"",
    "format:repo": "npm run format && npm run lint -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "jest --watch "
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.5.2",
    "@remix-run/css-bundle": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@testing-library/jest-dom": "^6.1.4",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@testing-library/react": "^14.1.0",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.1.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prisma": "^5.5.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
